<context name="teaching_ctx" type="TemporalContext">
<definition organization="engineering_school">(h&gt;=8 &amp; h&lt;=12) | (h&gt;=14 &amp; h&lt;=18)</definition>
</context>
<context name="computer_study_bs" type="BeanShellContext">
<definition organization="campus_B">import java.util.Date;
import java.util.Calendar;
// get hour
int hour = date.get(Calendar.HOUR_OF_DAY);
result = hour &gt;= 14 &amp;&amp; hour &lt;= 18;</definition>
<definition organization="campus_A">import java.util.Date;
import java.util.Calendar;
// get hour
int hour = date.get(Calendar.HOUR_OF_DAY);
result = hour &gt;= 8 &amp;&amp; hour &lt;= 12;</definition>
</context>
<context name="net_maintenance_ctx" type="TemporalContext">
<definition organization="campus_B">(h&gt;=16 &amp; h&lt;=18)</definition>
<definition organization="campus_A">(h&gt;=8 &amp; h&lt;=10) &amp; (dow#6 &amp; dow#7)</definition>
</context>
<context name="revocationCtx" type="ProvaContext">
<definition organization="university">hold(Org, Subject, Action, Object):-
    attribute(Object, grantor, Subject).</definition>
<definition organization="engineering_school">hold(Org, Subject, Action, Object):-
    attribute(Object, grantor, Subject).</definition>
</context>
<context name="teaching_ctx_bs" type="BeanShellContext">
<definition organization="engineering_school">import java.util.Date;
import java.util.Calendar;
// get hour
int hour = date.get(Calendar.HOUR_OF_DAY);
result = (hour &gt;= 8 &amp;&amp; hour &lt;= 12) || (hour &gt;= 14 &amp;&amp; hour &lt;= 18);</definition>
</context>
<context name="computer_study" type="TemporalContext">
<definition organization="campus_B">(h&gt;=14 &amp; h&lt;=18)</definition>
<definition organization="campus_A">(h&gt;=8 &amp; h&lt;=12)</definition>
</context>
<context name="net_maintenance_ctx_bs" type="BeanShellContext">
<definition organization="campus_B">import java.util.Date;
import java.util.Calendar;
// get hour
int hour = date.get(Calendar.HOUR_OF_DAY);
result = hour &gt;= 16 &amp;&amp; hour &lt;= 18;</definition>
<definition organization="campus_A">import java.util.Date;
import java.util.Calendar;
// get hour and day of week
int hour = date.get(Calendar.HOUR_OF_DAY);
int dayOfWeek = date.get(Calendar.DAY_OF_WEEK);
result = ( hour &gt;= 8 &amp;&amp; hour &lt;= 10 ) &amp;&amp; 
         ( dayOfWeek != Calendar.SATURDAY &amp;&amp;
           dayOfWeek != Calendar.SUNDAY );</definition>
</context>
<context name="network_emergency" type="UserDefinedContext">
<definition organization="engineering_school">false</definition>
</context>
<view_definition abstract_entity="teaching_delegation_view" name="view_definition_delegation" type="CProvaEntityDefinition">
<definition organization="engineering_school">bind(Org, Entity, AbstractEntity):-
	attribute(Entity, privilege, teaching),
	attribute(Entity, target, student_class),
	attribute(Entity, grantee, Grantee),
	empower(university, Grantee, replacement_teacher),
	attribute(Grantee, experience, Xp),
	greaterEq(Xp, 4).</definition>
</view_definition>
<view_definition abstract_entity="replacement_teacher_role_delegation" name="view_definition_role_deleg" type="CProvaEntityDefinition">
<definition organization="university">bind(Org, Entity, AbstractEntity):-
    attribute(Entity, assignee, Assignee),
    empower(Org, Assignee, phd_student).</definition>
</view_definition>
<view_definition abstract_entity="use_teaching_resource_assignment_view" name="view_definition_example" type="CProvaEntityDefinition">
<definition organization="engineering_school">bind(Org, Entity, AbstractEntity):-
    attribute(Entity, assignee, Action),
    attribute(Action, student_software, true).</definition>
</view_definition>
